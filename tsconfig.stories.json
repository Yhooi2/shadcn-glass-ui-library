{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.stories.tsbuildinfo",
    "noEmit": true
  },
  "include": ["src/**/*.stories.ts", "src/**/*.stories.tsx", "src/**/*.tsx", "src/**/*.ts"],
  "exclude": [
    "src/**/__tests__/**",
    "src/**/*.test.ts",
    "src/**/*.test.tsx",
    "src/**/*.spec.ts",
    "src/**/*.spec.tsx",
    "src/**/*.visual.test.tsx"
  ]
}
